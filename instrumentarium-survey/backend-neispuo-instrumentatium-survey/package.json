    "devDependencies": {
        "@nestjs/cli": "^6.9.0",
        "@nestjs/schematics": "^6.7.0",
        "@nestjs/testing": "^6.7.1",
        "@types/bcryptjs": "^2.4.2",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.1",
        "@types/flat": "^5.0.1",
        "@types/hashids": "^1.0.30",
        "@types/inquirer": "^7.3.0",
        "@types/jest": "^24.0.18",
        "@types/jsonwebtoken": "^8.3.5",
        "@types/md5-file": "^4.0.2",
        "@types/node": "^12.7.5",
        "@types/supertest": "^2.0.8",
        "@types/xlsx": "0.0.36",
        "artillery": "^2.0.0-10",
        "husky": "^3.1.0",
        "jest": "^24.9.0",
        "lint-staged": "^9.4.3",
        "prettier": "^1.19.1",
        "sqlstring": "^2.3.1",
        "supertest": "^4.0.2",
        "ts-jest": "^24.1.0",
        "ts-loader": "^6.1.1",
        "ts-node": "^8.10.2",
        "tsconfig-paths": "^3.9.0",
        "tslint": "^5.20.0",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "^3.6.3",
        "uuid": "^3.4.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.ts": [
            "prettier --write",
            "npx tslint --fix",
            "git add"
        ]
    }
}
