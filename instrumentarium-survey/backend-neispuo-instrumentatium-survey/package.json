{
    "name": "backend-neispuo-survey",
    "version": "1.0.0-next.18",
    "description": "",
    "author": "",
    "license": "MIT",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "tslint -p tsconfig.json -c tslint.json --fix",
        "test": "jest --config ./jest.json",
        "test:watch": "npm run test -- --watch",
        "test:cov": "npm run test -- --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
        "db:init": "npm run build && node ./scripts/init-database.script.js && npm run db:migrate && npm run db:seed",
        "db:migrate": "ts-node ./node_modules/typeorm/cli.js migration:run -c default",
        "db:rollback-one:migration": "ts-node ./node_modules/typeorm/cli.js migration:revert -c default",
        "db:rollback-one:seed": "ts-node ./node_modules/typeorm/cli.js migration:revert -c app-seed",
        "db:seed": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed -c app-seed",
        "db:migration:generate": "npm run build && npm run typeorm migration:generate -- -c default -n rename-this-and-class-inside",
        "db:seed:generate": "npm run typeorm migration:create -- -c default -n Seed",
        "send-mail:new-access": "npx ts-node -r tsconfig-paths/register ./scripts/mail-access-dispatcher/send-access-mail"
    },
    "dependencies": {
        "@nest-modules/mailer": "^1.1.3",
        "@nestjs/common": "^6.11.11",
        "@nestjs/core": "^6.11.11",
        "@nestjs/platform-express": "^6.11.11",
        "@nestjs/schedule": "^0.4.0",
        "@nestjs/swagger": "^3.1.0",
        "@nestjs/typeorm": "^6.2.0",
        "@nestjsx/crud": "^4.6.2",
        "@nestjsx/crud-request": "^5.0.0-alpha.3",
        "@nestjsx/crud-typeorm": "^4.6.2",
        "@types/cron": "^1.7.3",
        "@types/lodash": "^4.14.155",
        "@types/lodash.clonedeep": "^4.5.6",
        "@types/md5": "^2.1.33",
        "@types/multer": "^1.4.0",
        "@types/nodemailer": "^6.4.0",
        "@types/randomstring": "^1.1.6",
        "@types/uuid": "^8.3.4",
        "bcryptjs": "^2.4.3",
        "class-transformer": "^0.2.3",
        "class-validator": "^0.11.0",
        "crypto-random-string": "^3.2.0",
        "csv-string": "^3.1.8",
        "dotenv": "^8.2.0",
        "flat": "^5.0.2",
        "flatten": "^1.0.3",
        "hasha": "^5.1.0",
        "hashids": "^2.2.1",
        "helmet": "^5.0.2",
        "inquirer": "^7.3.3",
        "jsonwebtoken": "^8.5.1",
        "jwks-rsa": "^2.0.4",
        "knex": "^0.20.2",
        "lodash.clonedeep": "^4.5.0",
        "md5-file": "^5.0.0",
        "moment": "^2.29.1",
        "mssql": "^6.2.3",
        "multer": "^1.4.2",
        "mysql": "^2.17.1",
        "nest-winston": "^1.3.4",
        "nodemailer": "^6.4.2",
        "password-validator": "^5.0.3",
        "randomstring": "^1.1.5",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.0",
        "rxjs": "^6.5.3",
        "swagger-ui-express": "^4.1.2",
        "typeorm": "^0.2.37",
        "typeorm-seeding": "^1.6.1",
