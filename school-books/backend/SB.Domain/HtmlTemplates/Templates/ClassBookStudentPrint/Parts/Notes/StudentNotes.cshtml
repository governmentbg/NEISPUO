@using SB.Domain
@model StudentNotesModel

<div class="page">
  <h3 class="page-header">Бележки</h3>

  <table>
    <thead>
      <tr>
        <th>Дата на създаване</th>
        <th>Създадена от</th>
        <th>Описание</th>
      </tr>
    </thead>
    <tbody>
      @foreach (var note in Model.Notes)
      {
        <tr>
          <td>@note.DateCreated</td>
          <td>@note.CreatedBySysUserName</td>
          <td>@note.Description</td>
        </tr>
      }
    </tbody>
  </table>
</div>
