@using SB.Domain
@model Student_Book_DPLRModel

<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8"/>
    @{ await IncludeAsync("ClassBookPrint.Parts._Styles", null); }
  </head>
  <body>
    @{ await IncludeAsync("ClassBookStudentPrint.Parts.CoverPage.StudentCoverPageDPLR", Model.CoverPageModel); }

    @if (Model.NotesModel.Notes.Any())
    {
      await IncludeAsync("ClassBookStudentPrint.Parts.Notes.StudentNotes", Model.NotesModel);
    }

    @{ await IncludeAsync("ClassBookStudentPrint.Parts.FooterPage.StudentFooterPage"); }
  </body>
</html>
