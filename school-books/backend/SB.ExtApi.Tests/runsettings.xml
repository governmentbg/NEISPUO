<?xml version="1.0" encoding="utf-8" ?>
<RunSettings>
  <DataCollectionRunSettings>
    <DataCollectors>
      <DataCollector friendlyName="XPlat code coverage">
        <Configuration>
          <!-- see https://github.com/coverlet-coverage/coverlet -->
          <Format>cobertura</Format>
          <Include>[SB.Domain]*</Include>
        </Configuration>
      </DataCollector>
    </DataCollectors>
  </DataCollectionRunSettings>
  <LoggerRunSettings>
    <Loggers>
      <Logger friendlyName="xunit" enabled="True">
      </Logger>
    </Loggers>
  </LoggerRunSettings>
</RunSettings>
