              "projects/student/src/manifest.webmanifest",
              "projects/student/src/student-ngsw-worker.js"
            ],
            "styles": [
              "projects/student/src/styles.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "styles"
              ]
            },
            "scripts": [],
            "allowedCommonJsDependencies": [
              "@fortawesome/pro-solid-svg-icons",
              "@fortawesome/pro-duotone-svg-icons",
              "@fortawesome/pro-regular-svg-icons",
              "fast-sha256",
              "error-stack-parser",
              "stack-generator",
              "stacktrace-gps",
              "stacktrace-js",
              "beaver-logger"
            ],
            "serviceWorker": true,
            "ngswConfigPath": "projects/student/ngsw-config.json"
          },
          "configurations": {
            "production": {
              "optimization": {
                "scripts": true,
                "styles": {
                  "minify": true,
