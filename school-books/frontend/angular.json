{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": false,
    "schematicCollections": [
      "@angular-eslint/schematics"
    ]
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "teacher": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "projects/teacher",
      "sourceRoot": "projects/teacher/src",
      "prefix": "sb",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "baseHref": "/t/",
            "outputPath": "dist/teacher",
            "index": "projects/teacher/src/index.html",
            "main": "projects/teacher/src/main.ts",
            "polyfills": "projects/teacher/src/polyfills.ts",
            "tsConfig": "projects/teacher/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/teacher/src/favicon.ico",
              "projects/teacher/src/assets",
              "projects/teacher/src/silent-refresh.html",
              "projects/teacher/src/manifest.webmanifest"
            ],
            "styles": [
              "projects/teacher/src/styles.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "styles"
              ]
            },
            "scripts": [],
            "allowedCommonJsDependencies": [
              "@uppy/core",
              "@uppy/xhr-upload",
              "@uppy/locales/lib/bg_BG",
              "@fortawesome/pro-solid-svg-icons",
              "@fortawesome/pro-duotone-svg-icons",
              "@fortawesome/pro-regular-svg-icons",
              "fast-sha256",
              "error-stack-parser",
              "stack-generator",
              "stacktrace-gps",
              "stacktrace-js",
              "beaver-logger"
            ],
            "serviceWorker": true,
            "ngswConfigPath": "projects/teacher/ngsw-config.json"
          },
          "configurations": {
            "production": {
              "optimization": {
                "scripts": true,
                "styles": {
                  "minify": true,
                  "inlineCritical": false
                },
                "fonts": false
              },
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/teacher/src/environments/environment.ts",
                  "with": "projects/teacher/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all",
              "sourceMap": true,
              "statsJson": false
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true,
              "statsJson": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "teacher:build:production"
            },
            "development": {
              "browserTarget": "teacher:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "browserTarget": "teacher:build",
            "proxyConfig": "proxy.conf.json"
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "teacher:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/teacher/src/test.ts",
            "polyfills": "projects/teacher/src/polyfills.ts",
            "tsConfig": "projects/teacher/tsconfig.spec.json",
            "karmaConfig": "projects/teacher/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/teacher/src/favicon.ico",
              "projects/teacher/src/assets",
              "projects/teacher/src/manifest.webmanifest"
            ],
            "styles": [
              "projects/teacher/src/styles.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "styles"
              ]
            },
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/teacher/**/*.ts",
              "projects/teacher/**/*.html"
            ]
          }
        }
      }
    },
    "student": {
