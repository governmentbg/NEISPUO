{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve teacher --open",
    "start-student": "ng serve student --open",
    "build": "cross-env NODE_ENV=production ng build",
    "build:dev": "ng build --configuration development",
    "show-bundle": "webpack-bundle-analyzer ./dist/teacher/stats.json",
    "show-map": "source-map-explorer ./dist/teacher/*.js",
    "test": "ng test",
    "test:ci": "ng test teacher --no-watch --no-progress --browsers=ChromeHeadlessCI && ng test student --no-watch --no-progress --browsers=ChromeHeadlessCI",
    "lint": "prettier --check . && ng lint teacher && ng lint student && ng lint shared",
    "lint-fix": "prettier --write . && ng lint teacher --fix && ng lint student --fix",
    "openapi-generator-docker": "cross-var docker run --rm -v $INIT_CWD:/local openapitools/openapi-generator-cli:v5.3.0",
    "openapi-generator-generate": "npm run openapi-generator-docker -- generate -i http://host.docker.internal:5001/swagger/v1/swagger.json -g typescript-angular -o /local/projects/sb-api-client/src -t /local/projects/sb-api-client/templates --additional-properties=ngVersion=12.1.3,supportsES6=true,sortModelPropertiesByRequiredFlag=false,useSingleRequestParameter=true,removeOperationIdPrefix=true --type-mappings=DateTime=Date,Date=Date",
    "openapi-generator-generate-debug": "npm run openapi-generator-generate -- --global-property debugModels=true --global-property debugOperations=true",
    "openapi-generator-templates": "npm run openapi-generator-docker -- author template -g typescript-angular -o /local/projects/sb-api-client/templates",
    "generate-api-remove-unused": "cd ./projects/sb-api-client/src && rimraf README.md index.ts api.module.ts git_push.sh .openapi-generator-ignore .gitignore api/api.ts model/models.ts",
    "generate-api": "rimraf ./projects/sb-api-client/src && npm run openapi-generator-generate && npm run generate-api-remove-unused",
    "generate-templates": "rimraf ./projects/sb-api-client/templates && npm run openapi-generator-templates"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.3.0",
    "@angular/cdk": "^14.1.1",
    "@angular/common": "^14.3.0",
    "@angular/compiler": "^14.3.0",
    "@angular/core": "^14.3.0",
    "@angular/forms": "^14.3.0",
    "@angular/material": "^14.1.1",
    "@angular/platform-browser": "^14.3.0",
    "@angular/platform-browser-dynamic": "^14.3.0",
    "@angular/router": "^14.3.0",
    "@angular/service-worker": "^14.3.0",
    "@babel/core": "^7.18.10",
    "@babel/helper-explode-assignable-expression": "^7.18.6",
    "@fortawesome/angular-fontawesome": "^0.10.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/pro-duotone-svg-icons": "github:CielaNorma/fontawesome-pro#fortawesome-pro-duotone-svg-icons-v5.12.0-gitpkg",
    "@fortawesome/pro-regular-svg-icons": "github:CielaNorma/fontawesome-pro#fortawesome-pro-regular-svg-icons-v5.12.0-gitpkg",
    "@fortawesome/pro-solid-svg-icons": "github:CielaNorma/fontawesome-pro#fortawesome-pro-solid-svg-icons-v5.12.0-gitpkg",
    "@ng-select/ng-select": "^9.0.2",
    "@types/zalgo-promise": "^1.0.0",
    "@uppy/angular": "^0.3.1",
    "@uppy/locales": "^2.1.1",
    "@uppy/xhr-upload": "^2.1.2",
    "angular-oauth2-oidc": "^13.0.1",
    "beaver-logger": "^4.0.34",
    "date-fns": "^2.23.0",
    "jwt-decode": "^3.1.2",
    "ng-dynamic-component": "^10.2.1",
    "regjsgen": "^0.8.0",
    "rxjs": "~6.6.7",
    "stacktrace-js": "^2.0.2",
    "tslib": "^2.2.0",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.1.1",
    "@angular-eslint/builder": "14.0.2",
    "@angular-eslint/eslint-plugin": "14.0.2",
    "@angular-eslint/eslint-plugin-template": "14.0.2",
    "@angular-eslint/schematics": "14.0.2",
    "@angular-eslint/template-parser": "14.0.2",
    "@angular/cli": "^14.1.1",
    "@angular/compiler-cli": "^14.3.0",
    "@types/jasmine": "^4.0.1",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "cross-env": "^7.0.3",
    "cross-var": "^1.1.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsdoc": "^38.1.4",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "jasmine-core": "~4.0.1",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ng-packagr": "^14.1.0",
    "prettier": "^2.3.2",
    "prettier-plugin-organize-imports": "^2.3.3",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "source-map-explorer": "^2.5.2",
    "tailwindcss": "^3.0.23",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "webpack-bundle-analyzer": "^4.4.2"
  },
  "overrides": {
    "stylus": "0.0.1-security"
  }
}
